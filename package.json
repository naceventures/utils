      "prettier": "^3.5.1",
      "shx": "^0.3.4",
      "tsup": "^8.3.6",
      "tsx": "^4.19.2",
      "typescript": "^5.7.3",
      "typescript-eslint": "^8.24.0",
      "vitest": "^3.0.5"
   },
   "dependencies": {
      "@js-temporal/polyfill": "^0.4.4",
      "@oslojs/crypto": "^1.0.1",
